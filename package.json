{
  "name": "extensions-showcase-pt",
  "private": true,
  "type": "module",
  "scripts": {
    "shopify": "shopify",
    "prisma": "prisma",
    "vite": "vite",
    "build": "remix vite:build",
    "dev": "shopify app dev --config development --store decjuba-development.myshopify.com",
    "start": "remix-serve ./build/server/index.js",
    "setup": "prisma generate && prisma migrate deploy",
    "docker-start": "npm run setup && npm run remix-serve",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "graphql-codegen": "graphql-codegen",
    "generate": "shopify app generate",
    "deploy": "shopify app deploy",
    "config:link": "shopify app config link",
    "config:push": "shopify app config push",
    "config:use": "shopify app config use",
    "env": "shopify app env",
    "reset": "shopify app dev --reset",
    "deploy:development": "shopify app deploy --config development --force",
    "deploy:staging": "shopify app deploy --config staging --force",
    "deploy:production": "shopify app deploy --config production --force"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "@remix-run/dev": "^2.15.3",
    "@remix-run/node": "^2.15.3",
    "@remix-run/react": "^2.15.3",
    "@remix-run/serve": "^2.15.3",
    "@shopify/cli": "^3.74.1",
    "@shopify/app-bridge": "^3.7.10",
    "@shopify/app-bridge-react": "^4.1.6",
    "@shopify/ui-extensions-react": "^2025.1.3",
    "@shopify/discount-app-components": "^3.0.1",
    "@shopify/polaris": "^13.9.2",
    "@shopify/polaris-icons": "^9.3.0",
    "@shopify/react-form": "^2.7.3",
    "@shopify/shopify-api": "^11.8.1",
    "@shopify/shopify-app-remix": "^3.7.1",
    "@shopify/shopify-app-session-storage-prisma": "^6.0.1",
    "isbot": "^5.1.22",
    "prisma": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "^2.15.3",
    "@shopify/api-codegen-preset": "^1.1.4",
    "@shopify/app-bridge-types": "^0.0.16",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "vite": "^6.1.1"
  },
  "workspaces": [
    "extensions/*"
  ],
  "trustedDependencies": [
    "@shopify/plugin-cloudflare"
  ]
}